<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <httpErrors errorMode="Detailed" />
    <rewrite>
      <rules>
        <rule name="Redirect for keyboard-downloads" stopProcessing="true">
          <match url=".*" />
          <action type="Redirect" url="https://keyman.com/keyboards?embed=macos" appendQueryString="true" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>